#include <stdio.h>    
int main()    
{  
    int n,i,j,count=0,arr[100];
    scanf("%d",&n);
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {    
        for(j=i+1;j<n;j++) 
        {  
            if (i!=j)
            {
		       if(arr[i]==arr[j])
              {
                 count++;
              }
            }
        }  
    }
        if(count==0)    
                printf("unique");
                else
                printf("duplicate");
            
    return 0;    
}  
