#include <stdio.h>
#include<string.h>
int main()
{
    char s1[20],s2[20];
    int i,j,n,temp1,temp2,count=0;
    gets(s1);
    gets(s2);
    n=strlen(s1);
    n=strlen(s2);
    for(i=0;i<n-1;i++)
    {
        for(j=0;j<n-i-1;j++)
        {
            if(s1[j]>s1[j+1]) 
            {
                temp1=s1[j];
                s1[j]=s1[j+1];
                s1[j+1]=temp1;
            }
            if(s2[j]>s2[j+1]) 
            {
                temp2=s2[j];
                s2[j]=s2[j+1];
                s2[j+1]=temp2;
            }
        }
    }
    puts(s1);
    puts(s2);
    for(i=0;s1[i]!='\0';i++)
    {
        if(s1[i]==s2[i])
        count++;
    }
    if(count==n)
    printf("ANAGRAM");
    else
    printf("NOT ANAGRAM");
    return 0;
}
